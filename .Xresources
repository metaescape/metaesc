!!$HOME/.Xresources

!! dbi
Xft.dpi:98

/* Couleurs Tango */

URxvt*externalBorder: 0
URxvt*internalBorder: 2
URxvt*borderLess:     false
URxvt.iso14755:       false
URxvt.iso14755_52:    false
urxvt*depth:          32

URxvt.saveLines:      10000

!!-字间距 
URxvt*letterSpace:      -1
URxvt*skipBuiltinGlyphs:  true

!selection color
URxvt.highlightColor: #bdbdbd
URxvt.highlighTextColor: #424242
! URxvt.transparent: true
! URxvt.shading:     10
! URxvt.fading:      40
!! font
! URxvt.font:        
URxvt.boldfont:    xft:Monospace:style=Bold:size=12.5
URxvt*font:xft:Monospace:style=regular:size=12.5,xft:Noto Serif CJK SC:pixelsize=22

!!perl扩展
! URxvt.perl-ext-common: default,matcher,resize-font,url-select,keyboard-select 
! URxvt.colorUL: #8be9fd

!!浏览器
URxvt.url-select.launcher: google-chrome
URxvt.url-select.underline: true

!!font-resize(Ctrl-j,Ctrl-k)
! URxvt.resize-font.smaller:C--
! URxvt.resize-font.bigger: C-=


!! scroll behavior
URxvt.scrollBar:         false
URxvt.scrollTtyOutput:   false
URxvt.scrollWithBuffer:  true
URxvt.scrollTtyKeypress: true

URxvt.termName:         xterm-256color


